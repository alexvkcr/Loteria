<p id="notice"><%= notice %></p>

<h1>Listing Lotteries</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Contraseña</th>
      <th>Usuario</th>
      <th>Luckynumber</th>
      <th>Genero</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lotteries.each do |lottery| %>
      <tr>
        <td><%= lottery.email %></td>
        <td><%= lottery.contraseña %></td>
        <td><%= lottery.usuario %></td>
        <td><%= lottery.luckynumber %></td>
        <td><%= lottery.genero %></td>
        <td><%= link_to 'Show', lottery %></td>
        <td><%= link_to 'Edit', edit_lottery_path(lottery) %></td>
        <td><%= link_to 'Destroy', lottery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lottery', new_lottery_path %>
